@model IEnumerable<WebApi.Dto.StudentDto>

<main class="container">
    <p>
        <a href="/Student/Create">Create New Student</a>
    </p>
    <table class="table">
        <thead>
            <tr>
                <th>
                    Student Name
                </th>
                <th>
                    Birth Date
                </th>
                <th>
                    Email
                </th>
                <th>
                    Phone Number
                </th>
                <th>
                    GitHub link
                </th>
                <th>
                    Notes
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <th>
                    @item.Name
                </th>
                <th>
                    @item.BirthDate
                </th>
                <th>
                    @item.Email
                </th>
                <th>
                    @item.PhoneNumber
                </th>
                <th>
                    @item.GitHubLink
                </th>
                <th>
                    @item.Notes
                </th>
                <th>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a>|
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </th>
            </tr>
            }
        </tbody>
    </table>
</main>